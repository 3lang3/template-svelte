{
  "name": "svelte-typescript-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "validate": "svelte-check",
    "precommit": "lint-staged",
    "lint-staged:js": "eslint -c .eslintrc.js --ext .ts,.svelte ",
    "lint:fix": "eslint --cache --ext .ts,.svelte --format=pretty ./src",
    "lint:prettier": "prettier --write \"src/**/*.{ts,svelte}\" --end-of-line auto"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@umijs/fabric": "^2.5.7",
    "autoprefixer": "^10.2.5",
    "chalk": "^4.1.0",
    "eslint": "^7.21.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "husky": "^5.1.3",
    "less": "^4.1.1",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.8",
    "postcss-aspect-ratio-mini": "^1.1.0",
    "postcss-load-config": "^3.0.1",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-write-svg": "^3.0.1",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  },
  "lint-staged": {
    "**/*.{js,ts,.svelte}": "npm run lint-staged:js",
    "**/*.{js,ts,svelte,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "node scripts/verifyCommit.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  }
}
